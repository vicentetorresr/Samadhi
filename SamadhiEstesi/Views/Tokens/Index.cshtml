@model IEnumerable<SamadhiEstesi.Modelos.Token>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Token1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaExpiracion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPersonaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Token1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaExpiracion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPersonaNavigation.IdPersona)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdToken">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdToken">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdToken">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
